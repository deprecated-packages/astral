{
    "name": "symplify/astral",
    "type": "phpstan-extension",
    "description": "Toolking for smart daily work with AST",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "phpstan/phpstan": "^1.8.1",
        "phpstan/phpdoc-parser": "^1.7.0",
        "nikic/php-parser": "^4.14.0",
        "symplify/package-builder": "^11.2"
    },
    "require-dev": {
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0",
        "symplify/symplify-kernel": "^11.2",
        "symplify/easy-testing": "^11.2",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Astral\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Astral\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.2-dev"
        },
        "phpstan": {
            "includes": [
                "config/services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/composer-json-manipulator": "<11.1.3",
        "symplify/easy-coding-standard": "<11.1.3",
        "symplify/phpstan-rules": "<11.1.3",
        "symplify/easy-testing": "<11.1.3",
        "symplify/rule-doc-generator-contracts": "<11.1.3",
        "symplify/php-config-printer": "<11.1.3",
        "symplify/phpstan-extensions": "<11.1.3",
        "symplify/rule-doc-generator": "<11.1.3",
        "symplify/vendor-patches": "<11.1.3",
        "symplify/skipper": "<11.1.3",
        "symplify/smart-file-system": "<11.1.3",
        "symplify/symfony-static-dumper": "<11.1.3",
        "symplify/symplify-kernel": "<11.1.3",
        "symplify/monorepo-builder": "<11.1.3",
        "symplify/config-transformer": "<11.1.3",
        "symplify/easy-ci": "<11.1.3",
        "symplify/coding-standard": "<11.1.3",
        "symplify/autowire-array-parameter": "<11.1.3",
        "symplify/easy-parallel": "<11.1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
